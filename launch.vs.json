				"+sv_cheats 1"
			]
		},
		{
			"type": "dll",
			"name": "Local-KZ",
			"project": "CMakeLists.txt",
			"exe": "${env.CS2_BIN}",
			"args": [
				"-insecure",
				"-dedicated",
				"+game_mode 0",
				"+game_type 0",
				"+developer 5",
				"+map de_dust2",
				"+exec server.cfg",
				"+sv_cheats 1"
			]
		}
	]
